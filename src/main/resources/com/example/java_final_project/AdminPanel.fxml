      xmlns:fx="http://javafx.com/fxml/1">

    <Button fx:id="homeButton" text="Home Page"/>

    <Label fx:id="adminLabel" text="Admin Panel"/>

    <Button fx:id="addSurveyButton" text="Add Survey"/>

    <Button fx:id="manageQuestionsButton" text="Manage Questions"/>

    <Button fx:id="manageOptionsButton" text="Manage Options"/>

    <Button fx:id="viewVotesButton" text="View Votes"/>

    <Button fx:id="logoutButton" text="Logout"/>

</VBox>
