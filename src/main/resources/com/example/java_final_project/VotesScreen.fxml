<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="20" alignment="CENTER"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1">

    <TableView fx:id="votesTable">
        <columns>
            <TableColumn fx:id="voteIdCol" text="Vote ID"/>
            <TableColumn fx:id="surveyCol" text="Survey"/>
            <TableColumn fx:id="questionCol" text="Question"/>
            <TableColumn fx:id="optionCol" text="Option"/>
            <TableColumn fx:id="dateCol" text="Voted At"/>
        </columns>
    </TableView>

    <Button fx:id="backButton" text="Back"/>

</VBox>
